 <script>
 var indexI = 0;
 var indexJ = 0;

 function clickShirt(e) {
  var shirt = ["/images/shirt1.png","/images/shirt2.png","/images/shirt3.png"];
  var x = document.getElementById("Shirt");
  indexI += 1;
  indexI = indexI % 3;
  x.setAttribute( "src", shirt[indexI] );
 }

 function clickPants(e) {
  var pants = ["/images/pant1.png","/images/pant2.png","/images/pant3.png"];
  var y = document.getElementById("Pants");
  indexJ += 1;
  indexJ = indexJ % 3;
  y.setAttribute( "src", pants[indexJ] );
 }
 </script>
</head>

